<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.example.controller.GameController">
    <children>
        <Label fx:id="initMsg" text="Press ENTER key to start"/>
        <Circle fx:id="head" radius="10" fill="black" visible="false"/>
    </children>
</StackPane>
